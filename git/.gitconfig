[alias]
  export = "!sh -c 'git checkout-index -a -f --prefix=$1/' -"

[alias]
  amend = commit --amend --reuse-message=HEAD
  cdi = diff --cached
  cdif = diff --cached
  cdiff = diff --cached
  ci = commit -v
  cia = commit -a -v
  cl = log --all --graph --date=short --format='%C(yellow)%h%C(reset) %C(magenta)[%ad]%C(reset) %C(cyan)@%an%C(reset)%C(auto)%d%C(reset) %s'
  clog = log --all --graph --date=short --format='%C(yellow)%h%C(reset) %C(magenta)[%ad]%C(reset) %C(cyan)@%an%C(reset)%C(auto)%d%C(reset) %s'
  co = checkout
  cp = cherry-pick
  di = diff
  dif = diff
  fixup = commit --fixup
  gl = log --all --branches --graph
  glog = log --all --branches --graph
  ls = ls-files
  ref = reflog
  sdi = diff --staged
  sdif = diff --staged
  sdiff = diff --staged
  sl = log --all --branches --decorate --graph --oneline
  slog = log --all --branches --decorate --graph --oneline
  st = status --branch --short

[alias]
  backout = revert
  up = checkout
  update = checkout
  branch-remote = branch -r
  branches = branch -a
  branches-remote = branch -r
  forget = rm -r --cached
  rollback = reset --soft
  showconfig = config --list
  sum = show
  summary = show
  tags = tag
  unstage = reset HEAD --

[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = auto

[core]
  editor = vim
  excludesfile = ~/.gitignore
  fscache = true
  preloadindex = true
  quotepath = false

[diff]
  indentHeuristic = true

[github]
  user = sasaplus1

[user]
;  email = sasaplus1@gmail.com
;  name = sasaplus1
  useConfigOnly = true

[include]
  path = ~/.gitconfig.local

[push]
  default = nothing

[rebase]
  autosquash = true

[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true

